2816. Double a Number Represented as a Linked List

https://leetcode.com/problems/double-a-number-represented-as-a-linked-list/submissions/1906387677/

class Solution {
    public ListNode rev(ListNode head){
        ListNode prev=null;
        while(head!=null){
            ListNode next=head.next;
            head.next=prev;
            prev=head;
            head=next;
        }
        return prev;
    }
    public ListNode doubleIt(ListNode head) {
        head=rev(head);
        ListNode temp=head;
        int c=0;
        while(temp!=null){
            int val=temp.val*2+c;
            temp.val=val%10;
            c=val/10;
            if(temp.next==null && c>0){
                temp.next=new ListNode(c);
                break;
            }
            temp=temp.next;
        }
        return rev(head);
    }
}

